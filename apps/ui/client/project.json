{
  "projectType": "application",
  "root": "apps/ui/client",
  "sourceRoot": "apps/ui/client/src",
  "prefix": "boochat",
  "targets": {
    "build": {
      "executor": "@nrwl/angular:webpack-browser",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/apps/ui/client",
        "index": "apps/ui/client/src/index.html",
        "main": "apps/ui/client/src/main.ts",
        "polyfills": "apps/ui/client/src/polyfills.ts",
        "tsConfig": "apps/ui/client/tsconfig.app.json",
        "assets": ["apps/ui/client/src/favicon.ico", "apps/ui/client/src/assets", "apps/ui/client/src/web.config"],
        "styles": ["apps/ui/client/src/styles.css"],
        "scripts": [],
        "customWebpackConfig": {
          "path": "apps/ui/client/webpack.config.js"
        },
        "buildLibsFromSource": false
      },
      "configurations": {
        "production": {
          "fileReplacements": [
            {
              "replace": "apps/ui/client/src/environments/environment.ts",
              "with": "apps/ui/client/src/environments/environment.prod.ts"
            }
          ],
          "outputHashing": "none",
          "customWebpackConfig": {
            "path": "apps/ui/client/webpack.prod.config.js"
          }
        },
        "development": {
          "buildOptimizer": false,
          "optimization": false,
          "vendorChunk": true,
          "extractLicenses": false,
          "sourceMap": true
        }
      },
      "defaultConfiguration": "production"
    },
    "serve": {
      "executor": "@nrwl/angular:webpack-server",
      "configurations": {
        "production": {
          "browserTarget": "client:build:production"
        },
        "development": {
          "browserTarget": "client:build:development"
        }
      },
      "defaultConfiguration": "development",
      "options": {
        "port": 5000,
        "publicHost": "http://localhost:5000"
      }
    },
    "extract-i18n": {
      "executor": "@angular-devkit/build-angular:extract-i18n",
      "options": {
        "browserTarget": "client:build"
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": ["apps/ui/client/src/**/*.ts", "apps/ui/client/src/**/*.html"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/apps/ui/client"],
      "options": {
        "jestConfig": "apps/ui/client/jest.config.js",
        "passWithNoTests": true
      }
    },
    "serve-mfe": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "commands": ["nx serve events-ui", "nx serve chat-ui", "nx serve client"]
      }
    }
  },
  "tags": ["app"]
}
